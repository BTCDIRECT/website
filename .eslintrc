{

    "plugins": [
        "react"
    ],
     "extends": ["eslint:recommended", "google"],
     "parser": "babel-eslint",
    "parserOptions":
    {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures":
        {
            "jsx": true,
            "modules": true,
            "experimentalObjectRestSpread": true
        }
    },
    "rules": {
    "no-unused-vars": 0,
    "require-jsdoc": 0,
    "no-undef":0,
    "max-len": 1
  }
}